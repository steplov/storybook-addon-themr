{
  "name": "storybook-addon-themr",
  "version": "0.0.1-alpha.2",
  "description": "Storybook Addon for React CSS Themr Library",
  "main": "lib/index.js",
  "modules": "es/index.js",
  "scripts": {
    "start": "BABEL_ENV=commonjs start-storybook -p 9001",
    "build-storybook": "BABEL_ENV=commonjs build-storybook -o docs",
    "prepublish": "npm run build",
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib --ignore __tests__",
    "build:es": "BABEL_ENV=es babel src --out-dir es --ignore __tests__",
    "eslint": "eslint .",
    "test": "NODE_ENV=test jest",
    "clean": "rm -rf es lib storybook",
    "precommit": "npm run eslint && npm run test",
    "prepush": "npm run eslint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/steplov/react-storybook-addon-themr.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steplov/react-storybook-addon-themr/issues"
  },
  "homepage": "https://github.com/steplov/react-storybook-addon-themr",
  "devDependencies": {
    "@storybook/addon-info": "^3.0.1",
    "@storybook/addon-options": "^3.0.1",
    "@storybook/addons": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "husky": "^0.13.4",
    "postcss": "^6.0.1",
    "postcss-apply": "^0.8.0",
    "postcss-color-function": "^4.0.0",
    "postcss-css-variables": "^0.7.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-nested": "^2.0.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "react-toolbox": "^2.0.0-beta.12",
    "regenerator-runtime": "^0.10.5",
    "style-loader": "^0.18.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "@storybook/addon-actions": "^3.0.0",
    "@storybook/addon-info": "^3.0.0",
    "@storybook/addon-options": "^3.0.0",
    "@storybook/addons": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "@storybook/ui": "^3.0.0"
  },
  "dependencies": {
    "babel-jest": "^20.0.3",
    "jest": "^20.0.4",
    "react-css-themr": "^2.1.2"
  },
  "jest": {
    "rootDir": ".",
    "silent": false,
    "verbose": true,
    "testRegex": "/__tests__/.+spec.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/(node_modules|example|.storybook|.scripts)/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.scripts/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./test-reports",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 95,
        "statements": 95
      }
    }
  }
}
